<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<%@ taglib uri="/WEB-INF/struts-tiles.tld" prefix="tiles" %>
<%@ taglib uri="/WEB-INF/struts-html.tld" prefix="html" %>
<%@ taglib uri="/WEB-INF/struts-bean.tld" prefix="bean" %>
<%@ taglib uri="/WEB-INF/struts-logic.tld" prefix="logic" %>

<html>

<head>
    <title><tiles:getAsString name="title" /></title>

    <link REL="stylesheet" href="styles.css" type="text/css" />

    <meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1"></meta>
    <meta name="author" content="FireStorm/DAO"></meta>
    <meta name="email" content="firestorm@codefutures.com"></meta>
</head>

<body class="composite">

    <div id="banner">
        <table border="0" width="100%" cellpadding="8" cellspacing="0">
            <tr>
                <td>
<%--                    <tiles:insert attribute="header" />--%>
                </td>
            </tr>
        </table>
    </div>

    <div id="breadcrumbs">
        <table border="0" width="100%" cellpadding="4" cellspacing="0">
        <tr>
        <td><tiles:getAsString name="title" /></td>
        </tr>
        </table>
    </div>

    <table border="0" width="100%" cellpadding="8" cellspacing="0" id="layout">
        <tr valign="top">
            <td id="leftColumn">
                <div id="navBox">
                <div class="navSection">
                    <tiles:insert attribute="nav" />
                </div>
                </div>
            </td>
            <td rowspan="2" align="left">
                <div id="bodyColumn">
                <div class="contentBox">
                    <html:errors/>
                    <tiles:insert attribute="body" />
                </div>
                </div>
            </td>
        </tr>
    </table>

    <div id="footer">
        <table border="0" style="width:100%" cellpadding="4" cellspacing="0">
            <tr>
                <td>
                <p>Generated by <a href="http://www.codefutures.com/products/firestorm">FireStorm/DAO</a> from <a href="http://www.codefutures.com">CodeFutures</a>.</p>
                </td>
            </tr>
        </table>
    </div>

</body>
</html>

